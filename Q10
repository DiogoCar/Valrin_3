public class Valrin {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here


 int p=0;
  
 
 
   
   for(int i =1; i<=15;i++) {int n;String sn;
 
     
    sn=JOptionPane.showInputDialog(null, "Numero: ("+i+")");
    n= Integer.parseInt(sn);  
   
   if(n>0)p++;}
   
   
   
   JOptionPane.showMessageDialog(null,"Numeros positivos: "+p);
    }
   
}
