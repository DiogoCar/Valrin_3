package exercicio_fix;

import javax.swing.JOptionPane;
/**
 *
 * @author Discente
 */

public class Valrin {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
    int x,x1,y,y1;
    String sn,sm;
    
    sn=JOptionPane.showInputDialog(null, "Numero: ");
    sm=JOptionPane.showInputDialog(null, "Elevado a: ");
    x = Integer.parseInt(sn);   
    y = Integer.parseInt(sm); 
     
    
    x1=x;
    y1=1;
   
    
  
do{  y1=y1+1; x=x*x1;JOptionPane.showMessageDialog(null,x);} while (y1<y);
   
   


   
    }
   
}
