public class Valrin {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
    int x,y;
    
    x=0;
    y=30;
    
   do{ x=x+1; y=y-1;} while (x<10);
   
      JOptionPane.showMessageDialog(null,"X:"+x+" Y:"+y);
    }
   
}
